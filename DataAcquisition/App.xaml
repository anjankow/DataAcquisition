<Application x:Class="DataAcquisition.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:DataAcquisition"
             StartupUri="MainWindow.xaml">
    <Application.Resources>

        <Color x:Key="MainWin_backgnd">#FF192B3E</Color>
        <Color x:Key="OtherWin_backgnd">#FF2E434D</Color>
        <Color x:Key="LightYellow">#FFFAFFCB</Color>
        <Color x:Key="GreenLighter">#FFD0FFD9</Color>
        <Color x:Key="Green">#FFACE4B7</Color>
        <Color x:Key="LightRed">#FFD3746B</Color>
        <Color x:Key="Connected">#FF68B01F</Color>
        <Color x:Key="Disconnected">#FFD82F15</Color>
        <Color x:Key="NotImportantButton">#FFDEDEDE</Color>

        <Style TargetType="Button" x:Key="NotImportantBtnStyle">
            <Setter Property="Height" Value="28"/>
            <Setter Property="Width" Value="106"/>
            <Setter Property="Background" Value="#FFDEDEDE"/>
        </Style>

    </Application.Resources>
</Application>
